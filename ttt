/* ===== ИСТОЧНИКИ ===== */
(function() {
 Lampa.Controller.listener.follow('toggle', function(e) {
     if(e.name == 'select') {
       setTimeout(function() {var elementIZ = $('.selectbox-item.selector > div:contains("Изменить ссылку")'); if(elementIZ.length > 0) elementIZ.parent('div').hide();}, 10);}})
